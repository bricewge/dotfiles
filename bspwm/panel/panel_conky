\ab${exec scripts/panel_wifi}\ac${template0 wifi}\ae  \
\ab${exec scripts/panel_volume}\ac${template0 wifi}\ae  \
${exec scripts/panel_battery} \
\ab${time %a %H:%M }\ac${template0 cal}\ae
